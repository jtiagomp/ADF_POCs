<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="welcome">
    <page>/welcome.jsf</page>
  </view>
  <managed-bean id="__1">
    <managed-bean-name>LoginBean</managed-bean-name>
    <managed-bean-class>view.common.LoginBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__2">
    <managed-bean-name>NavigationBean</managed-bean-name>
    <managed-bean-class>view.common.NavigationBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__3">
    <managed-bean-name>SecurityContextAppSession</managed-bean-name>
    <managed-bean-class>view.common.SecurityContextApp</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
</adfc-config>
